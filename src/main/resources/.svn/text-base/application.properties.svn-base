# db config
#spring.datasource.username=kye
#spring.datasource.password=!kye123!
spring.datasource.username=root
spring.datasource.password=ok
spring.datasource.url = jdbc:mysql://localhost:3306/iocas_cloud?characterEnconding=utf8?useUnicode=true&characterEncoding=utf-8&useSSL=false

spring.datasource.driverClassName = com.mysql.jdbc.Driver

spring.datasource.initialSize=20
spring.datasource.minIdle=10
spring.datasource.maxActive=100
# 配置获取连接等待超时的时间
spring.datasource.tomcat.max-wait=60000
# 配置间隔多久才进行一次检测，检测需要关闭的空闲连接，单位是毫秒
spring.datasource.tomcat.time-between-eviction-runs-millis=60000
# 配置一个连接在池中最小生存的时间，单位是毫秒
spring.datasource.tomcat.min-evictable-idle-time-millis= 30000

